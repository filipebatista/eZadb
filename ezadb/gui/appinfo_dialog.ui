<?xml version='1.0' encoding='utf-8'?>
<interface>
    <object class="pygubu.builder.widgets.dialog" id="appinfo_dialog">
        <property name="height">640</property>
        <property name="modal">false</property>
        <property name="modal">True</property>
        <property name="resizable">both</property>
        <property name="title" translatable="yes">Application Info</property>
        <property name="width">480</property>
        <child>
            <object class="ttk.Notebook" id="notebook_info">
                <property name="height">200</property>
                <layout>
                    <property name="column">0</property>
                    <property name="propagate">True</property>
                    <property name="row">0</property>
                    <property name="sticky">nsew</property>
                    <rows>
                        <row id="0">
                            <property name="weight">1</property>
                        </row>
                    </rows>
                    <columns>
                        <column id="0">
                            <property name="weight">1</property>
                        </column>
                    </columns>
                </layout>
                <child>
                    <object class="ttk.Notebook.Tab" id="tab_fullinfo">
                        <property name="sticky">nsew</property>
                        <property name="text" translatable="yes">Full Info</property>
                        <child>
                            <object class="pygubu.builder.widgets.scrollbarhelper" id="scrolledframe_1">
                                <property name="scrolltype">both</property>
                                <property name="usemousewheel">true</property>
                                <layout>
                                    <property name="column">0</property>
                                    <property name="propagate">True</property>
                                    <property name="row">0</property>
                                    <property name="sticky">nsew</property>
                                    <rows>
                                        <row id="0">
                                            <property name="weight">1</property>
                                        </row>
                                    </rows>
                                    <columns>
                                        <column id="0">
                                            <property name="weight">1</property>
                                        </column>
                                    </columns>
                                </layout>
                                <child>
                                    <object class="tk.Text" id="text_fullinfo">
                                        <property name="state">disabled</property>
                                        <property name="wrap">word</property>
                                        <layout>
                                            <property name="column">0</property>
                                            <property name="propagate">True</property>
                                            <property name="row">0</property>
                                            <property name="sticky">nsew</property>
                                            <rows>
                                                <row id="0">
                                                    <property name="weight">1</property>
                                                </row>
                                            </rows>
                                            <columns>
                                                <column id="0">
                                                    <property name="weight">1</property>
                                                </column>
                                            </columns>
                                        </layout>
                                    </object>
                                </child>
                            </object>
                        </child>
                    </object>
                </child>
                <child>
                    <object class="ttk.Notebook.Tab" id="tab_lightinfo">
                        <property name="sticky">nsew</property>
                        <property name="text" translatable="yes">Light Info</property>
                        <child>
                            <object class="pygubu.builder.widgets.scrollbarhelper" id="scrolledframe_2">
                                <property name="scrolltype">both</property>
                                <property name="usemousewheel">true</property>
                                <layout>
                                    <property name="column">0</property>
                                    <property name="propagate">True</property>
                                    <property name="row">0</property>
                                    <property name="sticky">nsew</property>
                                    <rows>
                                        <row id="0">
                                            <property name="weight">1</property>
                                        </row>
                                    </rows>
                                    <columns>
                                        <column id="0">
                                            <property name="weight">1</property>
                                        </column>
                                    </columns>
                                </layout>
                                <child>
                                    <object class="tk.Text" id="text_lightinfo">
                                        <property name="state">disabled</property>
                                        <property name="wrap">word</property>
                                        <layout>
                                            <property name="column">0</property>
                                            <property name="propagate">True</property>
                                            <property name="row">0</property>
                                            <property name="sticky">nsew</property>
                                            <rows>
                                                <row id="0">
                                                    <property name="weight">1</property>
                                                </row>
                                            </rows>
                                            <columns>
                                                <column id="0">
                                                    <property name="weight">1</property>
                                                </column>
                                            </columns>
                                        </layout>
                                    </object>
                                </child>
                            </object>
                        </child>
                    </object>
                </child>
            </object>
        </child>
    </object>
</interface>
